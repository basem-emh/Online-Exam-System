@model string
<td>
    <a asp-action="Details" asp-route-id="@Model" class="btn btn-outline-warning">
        <i class="fas fa-eye"></i> 
    </a>
</td>
<td>
    <a asp-action="Edit" asp-route-id="@Model" class="btn btn-outline-success">
        <i class="fas fa-edit"></i> 
    </a>
</td>
<td>
    <button type="button" class="btn btn-outline-danger" data-bs-toggle="modal" data-bs-target="#deleteModal-@Model">
        <i class="fas fa-trash"></i>
    </button>
</td>